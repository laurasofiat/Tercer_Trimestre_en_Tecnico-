CREATE TABLE formulario (
  id_usuarios SERIAL PRIMARY KEY,
  nombre VARCHAR(50),
  apellido VARCHAR(50),
  direccion TEXT,
  telefono VARCHAR(20),
  correo_electronico VARCHAR(100),
  mensaje TEXT
);

